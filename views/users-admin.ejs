<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users</title>
</head>
<body>
    <h1>Users</h1>
    <br><a href="/admin/users">Show All Users</a>|
    <a href="/admin/games">Show All Games</a>|
    <a href="/adminLogout">Logout Admin</a>
    <table border="2">
        <thead>
            <tr>
                <th>Username</th>
                <th>Password</th>
                <th>Full Name</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Gender</th>
                <th>Email</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(e=>{ %>
                <tr>
                    <td><%=e.username%></td>
                    <td><%=e.password%></td>
                    <td><%=e.full_name%></td>
                    <td><%=e.first_name%></td>
                    <td><%=e.last_name%></td>
                    <td><%=e.gender%></td>
                    <td><%=e.email%></td>
                    <td><a href="/admin/users/<%=e.id%>/delete">Delete</a>|
                        <a href="/admin/users/<%=e.id%>/edit">Edit</a>|
                        <a href="/admin/users/<%=e.id%>/seeGames">See Games</a></td>
                </tr>
            <% })%>
        </tbody>
    </table>
</body>
</html>